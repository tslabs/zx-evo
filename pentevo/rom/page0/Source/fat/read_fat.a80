
;LAST UPDATE: 28.09.2011 savelij

;  
Wc_fat		EQU 0X00		; 
Getfzap		EQU 0X01		;  /
Ent_dir		EQU 0X02		;  /  
Openfil		EQU 0X03		; 
Readfil		EQU 0X04		;  
Getlong		EQU 0X05		;   
Positf		EQU 0X06		;    
Prvlegz		EQU 0X07		;  " "
Nxtlegz		EQU 0X08		;  " "
Find_file	EQU 0X09		;     ,  
Next_find	EQU 0X0A		;   
Load_file	EQU 0X0B		;  
Read0sector	EQU 0X0C		;   

; FAT
BUF_512		EQU DRV_VAR		;0X200  
TDIRCLS		EQU BUF_512+0X0200	;0X400   DIR
DIR_CEP		EQU TDIRCLS+0X0400	;0X100   
ERORDRV		EQU DIR_CEP+0X0100	;2     A
CAL_FAT		EQU ERORDRV+2		;1  FAT
MANYFAT		EQU CAL_FAT+1		;1  FAT-
BYTSSEC		EQU MANYFAT+1		;1    
ROOTCLS		EQU BYTSSEC+1		;4   ROOT 
ROOTZAP		EQU ROOTCLS+4		;2 -   ROOT 
SEC_FAT		EQU ROOTZAP+2		;4    
RSVDSEC		EQU SEC_FAT+4		;2   
STARTRZ		EQU RSVDSEC+2		;4  /
FRSTDAT		EQU STARTRZ+4		;4      BPB
SEC_DSC		EQU FRSTDAT+4		;4    /
CLS_DSC		EQU SEC_DSC+4		;4    /
FATSTR		EQU CLS_DSC+4		;4   FAT 
FILE_SZ		EQU FATSTR+4		;4    
KOLWCLS		EQU FILE_SZ+4		;4   
LST0ZAP		EQU KOLWCLS+4		;4   
LSTLOAD		EQU LST0ZAP+4		;4     
TEK_DIR		EQU LSTLOAD+4		;4   DIR
TEK_ZAP		EQU TEK_DIR+4		;2    DIR
SAVEZAP		EQU TEK_ZAP+2		;2   
TEK_LVL		EQU SAVEZAP+2		;1   
TEK_LEV		EQU TEK_LVL+1		;1    
FREECLS		EQU TEK_LEV+1		;8    
KOLFIND		EQU FREECLS+8		;2     
AFILCLS		EQU KOLFIND+2		;4     
FB_EXT		EQU AFILCLS+4		;B  8.3   
SAVE_IY		EQU FB_EXT+0X0B		;2      
ADR_MASK_EXT	EQU SAVE_IY+2		;2     
ADR_SKLAD	EQU ADR_MASK_EXT+2	;2   
MAKS_ADR	EQU ADR_SKLAD+2		;1      
TFILCLS		EQU ERORDRV+0X0100	;?   ,     

;      FAT
		EX AF,AF'
		EX (SP),HL
		LD A,(HL)
		INC HL
		EX (SP),HL
		ADD A,A
		PUSH IX
		PUSH IY
		LD (ERORDRV),SP
		PUSH HL
		LD HL,EXITDRV
		EX (SP),HL
		PUSH HL
		LD HL,TABLFAT
		ADD A,L
		LD L,A
		LD A,H
		ADC A,0
		LD H,A
		LD A,(HL)
		INC HL
		LD H,(HL)
		LD L,A
		EX AF,AF'
		EX (SP),HL
		RET

ERR_DRV		LD SP,(ERORDRV)
EXITDRV		POP IY
		POP IX
		RET

TABLFAT		DW WC_FAT		;00  FAT
		DW GETFZAP		;01   
		DW ENT_DIR		;02 /  
		DW OPENFIL		;03  
		DW READFIL		;04     
		DW GETLONG		;05   
		DW POSITF		;06 WORK FOR TEK POSIT
		DW PRVLEGZ		;07 PREV LEGAL OPISAT
		DW NXTLEGZ		;08 NEXT LEGAL OPISAT
		DW FIND_FILE		;09        ,  
		DW NEXT_FIND		;0A     
		DW LD_FILE		;0B    
		DW READ0SECTOR		;0C    

;/    
;  (512 )
READFIL		EXX
		LD E,0
		EXX
RD_ADD		PUSH AF
		PUSH HL
		EX AF,AF'
		LD A,(BYTSSEC)
		LD H,A
		PUSH DE
		CALL BCDE_A
		LD C,H
		LD HL,TFILCLS
		EX DE,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE
		EX DE,HL
		POP HL
		LD A,C
		LD B,0
		DEC A
		AND L
		JR Z,ADD2FI3-5
		EXX
		LD C,A
		LD B,0
		EXX
		LD L,A
		LD A,C
		SUB L
		LD H,A
		EX AF,AF'
		CP H
		JR C,$+3
		LD A,H
		POP HL
		PUSH HL
		CALL ADD2FI2+1
		LD B,C
		LD A,(BYTSSEC)
		LD C,A
		EX (SP),HL
		EXX
		LD BC,0
		EXX
ADD2FI3		POP HL
		POP AF
		SUB B
		RET Z
		LD B,0
		CP C
		JR Z,ADD2FI2+1
		INC B
		SUB C
		JR NC,$-2
		DEC B
		ADD A,C
		PUSH AF
		LD A,B
		AND A
		JR Z,ADD2FI2
		POP AF
ADD2FI1		PUSH AF
		PUSH BC
		PUSH HL
		LD A,C
		EX DE,HL
		CALL LOADZP
		BIT 7,B
		JP NZ,ERR_DRV
		EX (SP),HL
		PUSH HL
		PUSH AF
		CALL REALSEC
		EXX
		PUSH BC
		EXX
		POP HL
		ADD HL,DE
		EX DE,HL
		LD HL,0
		ADC HL,BC
		LD B,H
		LD C,L
		POP AF
		POP HL
		EX AF,AF'
		EXX
		LD A,E
		EXX
		AND A
		JR Z,ADD2FI5
		EX AF,AF'
;		CALL TO_DRV
;		DB 5
		RST 8
		DB Com_dev
		DB To_drv
		DB 5
		JR ADD2FI4

ADD2FI5		EX AF,AF'
;		CALL TO_DRV
;		DB 3
		RST 8
		DB Com_dev
		DB To_drv
		DB 3
ADD2FI4		AND A
		JP NZ,ERR_DRV
		POP DE
		POP BC
		DJNZ ADD2FI1+1
ADD2FI2		POP AF
		INC B
		AND A
		RET Z
		LD C,A
		XOR A
		JR ADD2FI1

; 
OPENFIL		CALL GETFZAP
		LD DE,20
		ADD HL,DE
		LD C,(HL)
		INC HL
		LD B,(HL)
		LD E,5
		ADD HL,DE
		LD E,(HL)
		INC HL
		LD D,(HL)
		INC HL
		PUSH BC
		PUSH DE
		CALL LOADZP
		LD (FILE_SZ),DE
		LD (FILE_SZ+2),BC
		POP DE
		POP BC
		LD HL,TFILCLS
OPENFI1		CALL SAVEZP
		PUSH HL
		CALL RDFATZP
		CALL LST_CLS
		POP HL
		JR NC,OPENFI1
		LD B,0XFF
		CALL SAVEZP
FILESEC		LD DE,(FILE_SZ)
		LD BC,(FILE_SZ+2)
		CALL HWOSTSC
		JP BCDE200

HWOSTSC		LD A,D
		AND 1
		LD H,A
		LD A,E
		AND A
		JR Z,$+3
		INC H
		LD E,0
		LD L,E
		BIT 0,H
		JR Z,$+3
		INC H
		RES 0,D
		ADD HL,DE
		EX DE,HL
		LD HL,0
		ADC HL,BC
		LD B,H
		LD C,L
		RET

;   
LOADLST		CALL CPNUMSC
		JR NZ,LOADLS1
		LD HL,BUF_512
		RET

LOADLS1		LD (LSTLOAD+2),BC
		LD (LSTLOAD),DE
		LD HL,BUF_512
		PUSH HL
;		CALL TO_DRV
;		DB 2
		RST 8
		DB Com_dev
		DB To_drv
		DB 2
		AND A
		JP NZ,ERR_DRV
		POP HL
		RET

;    
CPNUMSC		LD HL,LSTLOAD
		LD A,(HL)
		INC HL
		CP E
		RET NZ
		LD A,(HL)
		INC HL
		CP D
		RET NZ
		LD A,(HL)
		INC HL
		CP C
		RET NZ
		LD A,(HL)
		CP B
		RET

;  DIR
ENT_DIR		CALL GETFZAP
		BIT 4,A
		EX AF,AF'
		LD A,E
		EX AF,AF'
		LD E,A
		LD D,(HL)
		RET Z
		EXX
		EX AF,AF'
		BIT 7,A
		JR Z,ENT_DI2
		LD HL,TEK_LEV
		LD A,(HL)
		AND A
		EXX
		RET Z
		EXX
		DEC A
		LD (HL),A
		ADD A,A
		LD H,HIGH (DIR_CEP)
		LD L,A
		LD A,(HL)
		INC L
		LD H,(HL)
		LD L,A
		JR ENT_DI1

ENT_DI2		LD HL,TEK_LEV
		LD A,(HL)
		INC A
		CP 0X80
		EXX
		RET Z
		EXX
		LD (HL),A
		DEC A
		ADD A,A
		LD H,HIGH (DIR_CEP)
		LD L,A
		LD BC,(TEK_ZAP)
		LD (HL),C
		INC L
		LD (HL),B
ENT_DI1		EXX
		PUSH DE
		LD DE,20
		ADD HL,DE
		LD C,(HL)
		INC HL
		LD B,(HL)
		LD E,5
		ADD HL,DE
		LD E,(HL)
		INC HL
		LD D,(HL)
		LD A,B
		OR C
		OR D
		OR E
		JR NZ,ENT_DI0
		LD DE,(ROOTCLS)
		LD BC,(ROOTCLS+2)
ENT_DI0		LD (TEK_DIR),DE
		LD (TEK_DIR+2),BC
		CALL INIRTSC
		POP DE
		RET

;   
GETFZAP		LD BC,(TEK_ZAP)			;  
		LD A,B
		OR C
		JR Z,GETRZAP			;  =0,     
		PUSH BC				;  
		DEC BC				;   
		CALL GETRZAP			;
		POP BC				;  
		LD E,A				;      
GETRZAP		PUSH DE
		CALL RDDIRSC			;    
		POP DE				;  E
		INC A
		LD A,E				; /  
		RET Z				;      
		EX AF,AF'			
GETFZA1		LD A,C
		AND 0X0F			;      
		LD E,A
		LD D,0
		EX DE,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE			;   
		LD DE,0X0B
		LD A,(HL)			;    
		EX DE,HL
		ADD HL,DE
		LD L,(HL)			;   
		LD H,A				;    
		EX DE,HL
		CALL CP_TWOT			;     ".." 
		LD D,0				;   
		JR NZ,GETFZA2
		LD D,0X80			;    
GETFZA2		EX AF,AF'
		CP 0X0F				;    
		LD A,E				;    
		LD E,D				;    
		RET NZ
		INC E				;   
		RET

;  DIR   BC
; : A=0XFF-   DIR
RDDIRSC		PUSH BC
		LD D,B
		LD E,C
		LD BC,0
		LD A,0X10
		CALL BCDE_A
		LD A,E
		PUSH AF
		LD A,(BYTSSEC)
		PUSH AF
		CALL BCDE_A
		LD HL,TDIRCLS
		EX DE,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE
		CALL LOADZP
		BIT 7,B
		JR Z,RDDIRS3
		POP BC
		POP BC
		POP BC
		XOR A
		DEC A
		RET

RDDIRS3		CALL REALSEC
		POP AF
		DEC A
		LD L,A
		POP AF
		AND L
		LD L,A
		LD H,0
		ADD HL,DE
		EX DE,HL
		LD HL,0
		ADC HL,BC
		LD B,H
		LD C,L
		CALL LOADLST
		POP BC
		XOR A
		RET

;   DIR
;   ""   DIR
INIRTSC		PUSH AF
		LD HL,TDIRCLS
		CALL SAVEZP
		LD A,D
		OR E
		OR B
		OR C
		JR Z,LASTCLS
NEXTCLS		PUSH HL
		CALL RDFATZP
		CALL LST_CLS
		POP HL
		JR C,LASTCLS
		CALL SAVEZP
		JR NEXTCLS

LASTCLS		LD BC,0XFFFF
		CALL SAVEZP
		CALL POSTF02
		POP AF
		RET

;  1 
CP_ONET		LD A,(HL)
		CP 0X2E
		RET NZ
		INC HL
		LD A,(HL)
		DEC HL
		CP 0X20
		RET NZ
		XOR A
		RET

;  2 
CP_TWOT		LD A,(HL)
		CP 0X2E
		RET NZ
		INC HL
		LD A,(HL)
		DEC HL
		CP 0X2E
		RET NZ
		XOR A
		RET

POSITF		CP LOW ((ENDTPOS-TPOSITF)/2)
		RET NC
		ADD A,A
		LD DE,TPOSITF
		LD L,A
		LD H,0
		ADD HL,DE
		LD E,(HL)
		INC HL
		LD D,(HL)
		EX DE,HL
		JP (HL)

TPOSITF		DW POSTF00
		DW POSTF01
		DW POSTF02
		DW POSTF03
		DW POSTF04
		DW POSTF05
		DW POSTF06
		DW POSTF07
ENDTPOS

;   
POSTF00		LD HL,(TEK_ZAP)
		LD (SAVEZAP),HL
		RET

;   
POSTF01		LD HL,(SAVEZAP)
		LD (TEK_ZAP),HL
		RET

;  "B"  
POSTF03		LD A,B
		AND A
		RET Z
		PUSH BC
		CALL PRVLEGZ
		POP BC
		DJNZ $-5
		RET

;  "B"  
POSTF04		LD A,B
		AND A
		RET Z
		PUSH BC
		CALL NXTLEGZ
		POP BC
		DJNZ $-5
		RET

;  "" 
POSTF05		LD BC,0
		PUSH BC
		EXX
		POP BC
		EXX
		CALL RDDIRSC
		LD DE,0X20
KOL_ZA4		LD A,(HL)
		CP 0X2E
		JR NZ,KOL_ZA1+2
		INC HL
		LD A,(HL)
		DEC HL
		CP 0X2E
		JR Z,KOL_ZA1+2
		INC BC
		ADD HL,DE
		JR KOL_ZA4

KOL_ZA1		INC BC
		ADD HL,DE
		LD A,H
		CP HIGH (BUF_512)+2
		JR NZ,KOL_ZA2
		CALL RDDIRSC
		LD D,0
		AND A
		JR Z,KOL_ZA2
		CALL KOL_ZA3
		DEC A
		RET

KOL_ZA2		LD E,0X0B
		ADD HL,DE
		LD A,(HL)
		AND A
		SBC HL,DE
		LD E,0X20
		CP 0X0F
		JR Z,KOL_ZA1
		CP 8
		JR Z,KOL_ZA1
		LD A,(HL)
		CP 0XE5
		JR Z,KOL_ZA1
		AND A
		JR Z,KOL_ZA3
		EXX
		INC BC
		EXX
		JR KOL_ZA1

KOL_ZA3		LD D,B
		LD E,C
		EXX
		PUSH BC
		EXX
		POP BC
		XOR A
		RET

;  ""   "BC"
POSTF06		LD (TEK_ZAP),BC
		RET

;  "BC"   "" 
POSTF07		LD BC,(TEK_ZAP)
		RET

CPTEKZP		CALL RDDIRSC
		RET M
		LD A,C
		AND 0X0F
		LD L,A
		LD H,0
		LD DE,BUF_512
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE
		LD DE,0X0B
		ADD HL,DE
		LD A,(HL)
		SBC HL,DE
		LD E,(HL)
		AND A
		RET

;    0    "" 
POSTF02		LD BC,0
		LD (TEK_ZAP),BC
		CALL RDDIRSC
		CALL CP_ONET
		JR Z,NXTLEG1
		LD A,(HL)
		AND A
		RET Z
		JR NXTLEG1+1

NXTLEGZ		LD BC,(TEK_ZAP)
NXTLEG1		INC BC
		CALL CPTEKZP
		CP 0XFF
		RET Z
		CP 0X0F
		JR Z,NXTLEG1
		CP 8
		JR Z,NXTLEG1
		LD A,E
		AND A
		RET Z
		CP 0XE5
		JR Z,NXTLEG1
		LD (TEK_ZAP),BC
		RET

PRVLEGZ		LD BC,(TEK_ZAP)
		CALL CPTEKZP
		CALL CP_TWOT
		RET Z
PRVLEG1		DEC BC
		CALL CPTEKZP
		CP 0X0F
		JR Z,PRVLEG1
		CP 8
		JR Z,PRVLEG1
		LD A,E
		CP 0XE5
		JR Z,PRVLEG1
		LD (TEK_ZAP),BC
		RET

SAVEZP		LD (HL),E
		INC HL
		LD (HL),D
		INC HL
		LD (HL),C
		INC HL
		LD (HL),B
		INC HL
		RET

LOADZP		LD E,(HL)
		INC HL
		LD D,(HL)
		INC HL
		LD C,(HL)
		INC HL
		LD B,(HL)
		INC HL
		RET

;  FAT
WC_FAT		LD HL,0XFFFF
		LD (LSTLOAD),HL
		LD (LSTLOAD+2),HL
;		CALL COM_DEV
;		DB 2
		RST 8
		DB Com_dev
		DB Kol_vol
		LD HL,ADR_CAT-0X100
		LD (CAL_FAT),A
		LD E,D
		LD D,0
		EX DE,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE
		INC HL
		INC HL
		INC HL
		LD E,(HL)
		INC HL
		LD D,(HL)
		INC HL
		LD C,(HL)
		INC HL
		LD B,(HL)
		LD (STARTRZ),DE
		LD (STARTRZ+2),BC
		CALL LOADLST
		PUSH HL
		POP IX
		LD HL,0
		LD E,(IX+0X16)
		LD D,(IX+0X17)		;BPB_FATSZ16
		LD A,D
		OR E
		JR NZ,RDFAT01		;  FAT12/16 (BPB_FATSZ16=0)
		LD E,(IX+0X24)
		LD D,(IX+0X25)
		LD L,(IX+0X26)
		LD H,(IX+0X27)		;BPB_FATSZ32
					;    +36
RDFAT01		LD (SEC_FAT+2),HL
		LD (SEC_FAT),DE		;   FAT-
		LD HL,0
		LD E,(IX+0X13)
		LD D,(IX+0X14)		;BPB_TOTSEC16
		LD A,D
		OR E
		JR NZ,RDFAT02		;  FAT12/16 (BPB_TOTSEC16=0)
		LD E,(IX+0X20)
		LD D,(IX+0X21)
		LD L,(IX+0X22)
		LD H,(IX+0X23)		;BPB_TOTSEC32
					;    +32
RDFAT02		LD (SEC_DSC+2),HL
		LD (SEC_DSC),DE		;-   /

; ROOTDIRSECTORS
		LD E,(IX+0X11)
		LD D,(IX+0X12)		;BPB_ROOTENTCNT
		LD (ROOTZAP),DE
		LD BC,0
		LD H,B
		LD L,C
		LD A,D
		OR E
		JR Z,RDFAT03
		LD A,0X10
		CALL BCDE_A
		EX DE,HL

;  
;ROOTDIRSECTORS=((BPB_ROOTENTCNT*32)++(BPB_BYTSPERSEC-1))/BPB_BYTSPERSEC
; HL ROOTDIRSECTORS.  FAT32,  HL=0 

RDFAT03		PUSH HL			;ROOTDIRSECTORS
		LD A,(IX+0X10)		;BPB_NUMFATS
		LD (MANYFAT),A
		LD DE,(SEC_FAT)
		LD HL,(SEC_FAT+2)
		DEC A
		JR Z,RDFAT06		; FAT  1,  
RDFAT05		EX DE,HL
		ADD HL,HL
		EX DE,HL
		ADC HL,HL
		DEC A
		JR NZ,RDFAT05
RDFAT06		POP BC			;  FAT-  
		CALL HLDEPBC		; ROOTDIRSECTORS
		LD C,(IX+0X0E)
		LD B,(IX+0X0F)		;BPB_RSVDSECCNT
		LD (RSVDSEC),BC
		CALL HLDEPBC		; BPB_RESVDSECCNT
		LD (FRSTDAT),DE
		LD (FRSTDAT+2),HL	;    
		LD B,H
		LD C,L
		LD HL,SEC_DSC
		CALL BCDEHLM		;   -  
		LD A,(IX+0X0D)
		LD (BYTSSEC),A		;   
		CALL BCDE_A		;  -   
		LD (CLS_DSC),DE
		LD (CLS_DSC+2),BC	; -   

;		LD A,(CAL_FAT)
;		CP 0XFF			; FAT  ?
;		JR NZ,RDFAT04
;  FAT   MBR
		LD HL,(CLS_DSC)
		LD DE,(CLS_DSC+2)	;HLDE=   
		ADD HL,HL
		PUSH HL
		EX DE,HL
		ADC HL,HL		;HLDE=  *2
		PUSH HL
		LD B,H
		LD C,L			;BCDE=  *2
		CALL RASCHET
		LD A,1
		POP DE
		POP HL			;HLDE=  *2
		JR Z,RDFAT04
		ADD HL,HL
		EX DE,HL
		ADC HL,HL		;HLDE=  *4
		LD B,H
		LD C,L			;BCDE=  *4
		CALL RASCHET
		LD A,2
		JR Z,RDFAT04
		XOR A

RDFAT04		LD (CAL_FAT),A

; FAT12/16 ROOT =0
; FAT32    +44
