OUT_TEK_DIR	LD L,(IX+0X0A)			;  TR-DOS 
		LD H,(IX+0X0B)			;   
		LD E,(IX+7)			;    
		XOR A
		LD D,A
		SBC HL,DE			;      
		LD DE,ADR_CAT
		ADD HL,HL
		LD A,(FLAGS)
		AND 8
		JR NZ,OUT_TEK_FAT		;   FAT
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL			;   
		ADD HL,DE			;      
		LD C,(IX+2)
		DEC C
		DEC C				;  
		LD D,(IX+0)
		LD E,(IX+1)
		INC D
		INC D
		INC E				;    
FINDBAS3	LD B,8				;  
		PUSH DE
FINDBAS4	LD A,(HL)
		INC HL
		CALL PRT_SYM
		DJNZ FINDBAS4
		LD DE,8				;8  ,  8 
		ADD HL,DE
		POP DE
		INC E
		DEC C
		JR NZ,FINDBAS3			;       
		RET

;   FAT
OUT_TEK_FAT	ADD HL,DE
		LD D,(IX+0)
		LD E,(IX+1)
		INC D
		INC D
		INC E				;    
		LD C,0
KOL4OUT		EQU $-1
OTF2		PUSH BC
		LD C,(HL)
		INC HL
		LD B,(HL)
		INC HL
		PUSH HL
		PUSH DE
		LD A,6
		CALL COM_FAT
		DB Positf
		CALL COM_FAT
		DB Getfzap
		POP DE
		PUSH DE
		LD BC,0X0B
		ADD HL,BC
		LD A,(HL)
		SBC HL,BC
		AND 0X10
		LD A,0
		JR NZ,OTF4
		LD A,1
OTF4		CALL PRTSYMALT
		LD B,8
OTF1		LD A,(HL)
		INC HL
		CALL PRT_SYM
		DJNZ OTF1
		POP DE
		POP HL
		POP BC
		INC E
		DEC C
		JR NZ,OTF2
		RET

