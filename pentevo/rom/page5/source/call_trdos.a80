DISK_NONE	LD A,1
		OUT (PEVO_CONF),A
		LD A,PAGE_RAMDISK
		LD BC,WIN_P1
		OUT (C),A
		LD HL,0X7FFF
		LD A,(HL)
		DEC H
		CP "R"
		JR NZ,DISK_NONE1
		LD A,(HL)
		CP "D"
DISK_NONE1	EX AF,AF'
		LD A,0XFA
		OUT (C),A
		XOR A
		OUT (PEVO_CONF),A
		EX AF,AF'
		JP NZ,CREATE_TRDRAM		;  , 
		RET

