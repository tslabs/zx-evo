GLUBOOT		LD HL,FLAGS
		RES 3,(HL)			;  
		LD HL,ADR_CAT
		LD DE,8
		LD BC,0X0105
		CALL TO_DOS4BAS			; 9 
		LD A,(ADR_CAT+0XE7)
		CP 0X10				;   TR_DOS
		JP NZ,CMP_VIRTUAL;PRT_NONETRDOS		;,   TR-DOS
		LD HL,ADR_CAT
		LD DE,0
		LD BC,0X0905
		CALL TO_DOS4BAS			; TR-DOS 
		LD A,(ADR_CAT+0X8E7)
		CP 0X10				;   TR_DOS
		JP NZ,_STUPID
