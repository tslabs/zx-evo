		LD BC,8
		LDIR
		LD HL,0X3E08+0XA7
		LD DE,0XFFFF
		LD C,0XA8			;21 ᨬ UDG
		LDDR
		LD HL,RUN_BASIC
		LD DE,0X5D3B
		LD BC,ERUN_BASIC-RUN_BASIC
		LDIR				;    
		CALL MEMSET			;  
		CALL FOR_RUN
		EI
		JP 0X1B7D			;   

